{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "airbnb-base", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-plusplus": "off",
        "no-param-reassign": "off",
        "no-unused-vars": "off",
        "no-multi-assign": "off",
        "no-undef": "off",
        "no-shadow": "off",
        "func-names": "off",
        "eqeqeq": "off",
        "no-continue": "off",
        "class-methods-use-this": "off",
        "consistent-return": "off",
        "no-sequences": "off",
        "import/prefer-default-export": "off",
        "import/extensions": "off",
        "no-bitwise": "off"
    }
}
